{
  "name": "Browser Compatibility",
  "version": "1.0",
  "description": "Developer Tools extension for checking browser compatibility.",
  "author": "Meggin Kearney",
  "minimum_chrome_version": "22.0",
  "manifest_version": 2,
  
  "devtools_page": "devtools.html",

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["js/content.js"]
    }
  ],

  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
